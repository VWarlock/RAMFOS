tapePulse:	; Задержка на L
		dcr	l		
		jp	tapePulse

		; Высокий уровень
		mvi	a, 0Fh		
		sta	IO_KEYB_MODE	

		; Задержка на H	
tapePulse_0:	dcr	h		
		jp	tapePulse_0

		; Низкий уровень
		mvi	a, 0Eh		
		sta	IO_KEYB_MODE
		ret