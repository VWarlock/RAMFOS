; RAMFOS
; Нажатие на клавишу CAPS LOCK
;
; 2013-11-01 Дизассемблировано vinxru
;----------------------------------------------------------------------------

capsLock:	dcr	b
		jp	capsLock1
		 mov	a, h
		 cma
		 mov	h, a
		jmp	capsLockRet

; ---------------------------------------------------------------------------

capsLock1:	lda	v_capsLock
		xri	2
		sta	v_capsLock
		jmp	specialKeysRet
